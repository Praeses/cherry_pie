# Ignore bundler config
/.bundle
/.gems

# database.
/db/*.sqlite3
/db/schema.rb
/database.yml
/settings.yml
/settings.yml.*
/database.yml.bak

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/bin
/.sass-cache
RackMultipart.*

tags
*.swp
*.war

/.vagrant

# Ignore javascript generated for jasmine testing
public/javascripts/compiled/*

public/assets

# Since everyone is using rbenv and I (Micah) am using rvm
# I decided to use my own custom config for this project
# and my config uses mri ruby!  Because it is faster...
.rvmrc

# Keep Kate swap files out of commits
.swap.*

# Keep .DS_Store files our of commits
.DS_Store

